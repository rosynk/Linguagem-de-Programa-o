package fatec;

import javax.swing.JOptionPane;

public class LtEx0114 {
    public static void main (String []args){
        int ang1, ang2, ang3, soma;
        ang1 = Integer.parseInt(JOptionPane.showInputDialog("Informe o primeiro ângulo: "));
        ang2 = Integer.parseInt(JOptionPane.showInputDialog("Informe o segundo ângulo: "));
        soma = ang1 + ang2;
        ang3 = 180 - soma;
        System.out.println (ang3);
    }
    
}
