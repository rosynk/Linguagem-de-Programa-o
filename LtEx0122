package fatec;

import javax.swing.JOptionPane;

/*Receba 2 valores inteiros e diferentes. Mostre seus valores em ordem crescente.*/
public class LtEx0122 {
    public static void main(String[] args) {
        int numero1, numero2;
        numero1 = Integer.parseInt(JOptionPane.showInputDialog("Informe um número: "));
        numero2 = Integer.parseInt(JOptionPane.showInputDialog("Informe outro número: "));
        if (numero1>numero2){
            System.out.println(numero2+", "+numero1);
        } else{
            System.out.println(numero1+", "+numero2);
        }
    }
    
}
