import javax.swing.*;
/*Receba um número. Calcule e mostre os resultados da tabuada desse número*/
public class LtEx0134 {
    public static void main (String []args){
        double tabuada=1, i = 1;
        int numero = Integer.parseInt(JOptionPane.showInputDialog("Informe um número"));
        do {
            tabuada = i * numero;
            System.out.println(tabuada);
            i = i + 1;
        }while(i<=10);
    }
}
